{
  "name": "@lidqqq/pipe",
  "version": "1.0.2",
  "description": "A function to create piped function",
  "scripts": {
    "test": "npx jest",
    "clean": "rm -rf dist/ coverage/",
    "build": "npx run-s build:ts build:esbuild:*",
    "build:ts": "npx tsc --build tsconfig.json",
    "build:esbuild:cjs": "npx esbuild --bundle src/index.ts --outfile=dist/target/cjs.js --platform=node",
    "build:esbuild:esm": "npx esbuild --bundle src/index.ts --outfile=dist/target/esm.mjs --platform=node --format=esm",
    "build:esbuild:browser": "npx esbuild --bundle src/index.ts --outfile=dist/target/browser.js --platform=browser",
    "postbuild": "sed -i '' -e 's/@/./g' dist/src/index.d.ts dist/src/index.js",
    "prepublish:npm": "npm run clean && npm test && npm run build",
    "publish:npm": "npm publish --access=public ./",
    "coverage": "npx jest --coverage"
  },
  "devDependencies": {
    "esbuild": "0.8.34",
    "esm": "3.2.25",
    "jest": "26.6.3",
    "npm-run-all": "4.1.5",
    "ts-jest": "26.4.4",
    "tslib": "2.1.0",
    "typescript": "4.1.3"
  },
  "main": "./dist/target/cjs.js",
  "module": "./dist/target/esm.mjs",
  "browser": "./dist/target/browser.js",
  "exports": {
    ".": {
      "require": "./dist/target/cjs.js",
      "default": "./dist/target/esm.mjs"
    }
  },
  "types": "dist/src/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "pipe",
    "typescript"
  ],
  "author": "lidqqq <lidqqq.dev@gmail.com> (https://lidqqq.dev/)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lidqqq/pipe.git"
  },
  "bugs": {
    "url": "https://github.com/lidqqq/pipe/issues"
  },
  "homepage": "https://github.com/lidqqq/pipe#readme"
}
